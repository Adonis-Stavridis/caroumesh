{
  "name": "caroumesh",
  "license": "MIT",
  "version": "1.2.7",
  "type": "module",
  "author": "Adonis Stavridis",
  "description": "A carousel-like display to showcase your 3D models",
  "homepage": "https://github.com/Adonis-Stavridis/caroumesh",
  "repository": {
    "type": "git",
    "url": "https://github.com/Adonis-Stavridis/caroumesh"
  },
  "bugs": {
    "url": "https://github.com/Adonis-Stavridis/caroumesh/issues"
  },
  "keywords": [
    "react",
    "three",
    "react-three-fiber",
    "gltf",
    "glb",
    "carousel",
    "3D"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/caroumesh.es.js",
      "require": "./dist/caroumesh.umd.js"
    }
  },
  "main": "./dist/caroumesh.umd.js",
  "module": "./dist/caroumesh.es.js",
  "types": "./dist/components/index.d.ts",
  "scripts": {
    "dev": "yarn storybook",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}' --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --fix 'src/**/*.{jsx,ts,tsx}'",
    "format": "prettier --write src/**/*.{ts,tsx,scss} --config ./.prettierrc",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -o docs"
  },
  "dependencies": {
    "@react-three/drei": "^9.80.1",
    "@react-three/fiber": "^8.13.6",
    "bootstrap": "^5.3.1",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.8.0",
    "react-bootstrap-icons": "^1.10.3",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.10",
    "three": "^0.155.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.2.2-alpha.0",
    "@storybook/addon-interactions": "^7.2.2-alpha.0",
    "@storybook/addon-links": "^7.2.2-alpha.0",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.2.2-alpha.0",
    "@storybook/builder-vite": "^7.2.2-alpha.0",
    "@storybook/react": "^7.2.2-alpha.0",
    "@storybook/react-vite": "^7.2.2-alpha.0",
    "@storybook/testing-library": "^0.2.0",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@types/three": "^0.155.0",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@vitejs/plugin-react": "^4.0.4",
    "classnames": "^2.3.2",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "0.6.13",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.1",
    "react-hooks": "^1.0.1",
    "sass": "^1.64.2",
    "storybook": "7.2.1",
    "typescript": "*",
    "vite": "^4.4.8",
    "vite-plugin-css-injected-by-js": "^3.2.1",
    "vite-plugin-dts": "^3.5.1",
    "vite-plugin-linter": "^2.0.2",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "peerDependencies": {
    "@react-three/drei": "^9.80.1",
    "@react-three/fiber": "^8.13.6",
    "bootstrap": "^5.3.1",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.8.0",
    "react-bootstrap-icons": "^1.10.3",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.10",
    "three": "^0.155.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,scss}": [
      "yarn eslint",
      "yarn format"
    ]
  },
  "packageManager": "yarn@3.6.1"
}
